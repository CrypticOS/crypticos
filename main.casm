; CrypticOS Main Test Prompt

; Input var
var input 0

include "../kernel/kernel.casm"

out ">CrypticOS"
call newline

top:
	out ":"
	mov input getchar

	cmp input 'c' calc
	cmp input 'l' load
	cmp input 'e' exit

	out "Unknown"
	call newline
jmp top

calc:
	out "ascii add"
	call newline

	mov ga 3
	mov gb 2
	call k.mult
	add gr '0'
	out gr
	
	call newline
jmp top

load:
	out ""
jmp top

newline:
	inl "!**.+++."
ret

exit:

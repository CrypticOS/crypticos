; CrypticOS Main Test Prompt

var spaces '0'
var input 0

out "; Cryptic OS"
call newline

top:
	out ": "
	mov input getchar
	;call newline

	cmp input '?' help
	cmp input ' ' space
	out "UNKNOWN COMMAND"

	call newline
jmp top

help:
	out "YEP"
	call newline
jmp top

space:
	add spaces 1
	out "PRESSED:"
	out spaces
	call newline
jmp top

newline:
	inl "!**.+++."
ret

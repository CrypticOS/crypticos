; General purpose x86 registers
var ah, 0
var al, 0
var ch, 0
var cl, 0
var dh, 0
var dl, 0

mov al, 5 ; mode 12
mov ah, 0 ; 00h
call int ; int 0x10

call drawPixel

jmp end

drawPixel:
	mov ah, 12
	mov al, 5
	mov ch, 0
	mov cl, 10 ; x = 10
	mov dh, 0
	mov dl, 10 ; y = 10
	call int
ret

; Goto beginning
int:
	inl "<<<<<<<,>>>>>>"
ret

end:
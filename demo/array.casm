arr a 10

; In order to dynamically retrieve/set a certain cell in memory,
; we must include a kernel function in the program.
inc "$kernel/main.casm"
jmp a
inc "$kernel/mov.casm"
a:

; mem[gb] = gc
set ga SET
set gb &a
set gc 2
run k.mov

; gr = mem[gb]
set ga GET
set gb &a
run k.mov

; Print as digit
add gr 50
sub gr 2
prt gr

; Expect 2, the value we gave to a.

